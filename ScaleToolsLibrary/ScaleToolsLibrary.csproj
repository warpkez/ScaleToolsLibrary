<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <RepositoryUrl>https://github.com/warpkez/ScaleToolsLibrary</RepositoryUrl>
    <Version>1.0.3</Version>
    <PackageId>WarpKez.$(AssemblyName)</PackageId>
    <Authors>Wayne Jackson</Authors>
    <Product>WarpKez.$(AssemblyName)</Product>
    <Description>Helper library for scale modelling.  Coverting real world measurements to scale world.</Description>
  </PropertyGroup>

</Project>
